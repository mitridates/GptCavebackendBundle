{#
/**
 * @file tbody.html.twig
 * @parent ../index_ajax.html.twig
#}
{% for entity in entities%}
    <tr class="text-center-row">
        <td>{{ entity.getArticleid() }}</td>
        <td><a href="{{path('cave_backend_article_edit', { 'id': entity.getArticleid() })}}">{{ entity.getName() }}</a></td>
        <td>{{ entity.getAuthor()}}</td>
        <td>{{ entity.getPublicationyear()}}</td>
        <td>{%if entity.getCountry() is not null%}{{entity.getCountry().getName()}}{%endif%}</td>
        <td>{%if entity.getAdmin1() is not null%}{{entity.getAdmin1().getName()}}{%endif%}</td>
    </tr>
{% endfor %}