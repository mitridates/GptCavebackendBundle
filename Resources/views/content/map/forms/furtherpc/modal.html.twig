{% embed '@GptCavebackend/partial/page/panel.html.twig' with {type: arrayParams.page.style.form_panel}%}
{% block heading %}{{ ('map.form.'~name~'.tab')|trans({},'cavepages')|raw -}}{% endblock heading %}
{% block body %}
<table class="table table-bordered">
  <tr>
    <td>{{ form_label(form.country)}}</td>
  </tr>
  <tr>
    <td>{{ form_widget(form.country, { 'attr': {'class': 'countryidpcmodal'} })}}</td>
  </tr>
  <tr>
    <td >{{ form_label(form.admin1)}}</td>
  </tr>
  <tr>
    <td >{{ form_widget(form.admin1, { 'attr': {'class': 'admin1idpcmodal'} })}}</td>
  </tr>
  <tr>
    <td >{{ form_label(form.admin2)}}</td>
  </tr>
  <tr>
    <td >{{ form_widget(form.admin2, { 'attr': {'class': 'admin2idpcmodal'} })}}</td>
  </tr>
  <tr>
    <td >{{ form_label(form.position)}}</td>
  </tr>
  <tr>
    <td ><div class="col-lg-4 nopadding">{{ form_widget(form.position)}}</td>
  </tr>  
</table>
{% endblock body %}
{% endembed %}
{%include '@GptCavebackend/content/map/tabs/'~name~'/script.html.twig'%}