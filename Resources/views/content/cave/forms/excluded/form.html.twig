{#
/**
 * @file form.html.twig Caveexcluded form
 */
#}
<table class="table">
  <tr>
    <td>{{ form_label(form.excluded)}}</td>
    <td class="w-25">{{ form_label(form.position)}}</td>
  </tr>
  <tr>
    <td>{{ form_widget(form.excluded)}}</td>
    <td>{{ form_widget(form.position, {attr: {class: 'w-50'}})}}</td>
  </tr>
</table>
<script>
  $(document).ready(function() {
    $.fn.modal.Constructor.prototype._enforceFocus = function() {};
    $('#{{form.excluded.vars.id}}').select2({width: '100%'});
  });
</script>